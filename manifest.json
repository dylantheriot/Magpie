{
  "name": "Magpie",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://twitter.com/*", "https://twitter.com/*"],
      "js": ["jquery.js", "tweets.js", "jquery.withinviewport.js", "withinviewport.js"]
    }
  ],
  "web_accessible_resources": [
    "popup.html",
    "loading.html",
    "no_results.html",
    "magpie-btn.html"
  ]
}